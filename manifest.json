{
  "manifest_version": 3,
  "name": "Youtube Border",
  "version": "1.0.0",
  "description": "Add border to your Youtube video",
  "icons": {
    "16": "./icon.png",
    "32": "./icon.png",
    "64": "./icon.png",
    "128": "./icon.png"
  },
  "host_permissions": ["https://www.youtube.com/watch*"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch*"],
      "js": ["content.js"]
    }
  ]
}
